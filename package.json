{
  "name": "starry-code",
  "displayName": "StarryCode",
  "description": "A AI assistant for code",
  "version": "1.0.0",
  "publisher": "luunderstars",
  "author": {
    "name": "Lu Jianfu",
    "mail": "corleonelou@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tigerlove/starry"
  },
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "cursor",
    "ai",
    "rules",
    "snippets",
    "productivity"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "starry-code.openViewer",
        "title": "Open StarryCode CursorRules Viewer",
        "category": "Starry"
      },
      {
        "command": "starry-code.openSettings",
        "title": "Open StarryCode Settings",
        "category": "Starry"
      },
      {
        "command": "starry-code.focusChat",
        "title": "Focus StarryCode Chat",
        "category": "Starry"
      }
    ],
    "configuration": {
      "title": "Cursor Rules",
      "properties": {
        "cursorRules.cachedRules": {
          "type": "string",
          "default": "",
          "description": "Cached rules data"
        },
        "cursorRules.lastSync": {
          "type": "number",
          "default": null,
          "description": "Timestamp of last rules sync"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "starry-code",
          "title": "Starry Code",
          "icon": "assets/starry.svg"
        }
      ]
    },
    "views": {
      "starry-code": [
        {
          "type": "webview",
          "id": "starry-code.chatView",
          "name": "",
          "icon": "assets/starry.svg"
        }
      ]
    },
    "keybindings": [
      {
        "command": "starry-code.focusChat",
        "key": "ctrl+shift+[",
        "mac": "cmd+shift+["
      }
    ]
  },
  "scripts": {
    "install:all": "yarn install && cd webview-ui && yarn install",
    "start:webview": "cd webview-ui && yarn run start",
    "build:webview": "cd webview-ui && yarn run build",
    "vscode:prepublish": "yarn run build:webview && yarn run compile",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:webview": "cd webview-ui && yarn run watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "check-types": "tsc --noEmit",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "package": "npm run build:webview && npm run check-types && npm run lint && node esbuild.js --production",
    "vsce:package": "vsce package",
    "publish:marketplace": "vsce publish && ovsx publish",
    "sync-rules": "node scripts/sync-rules.js"
  },
  "devDependencies": {
    "@types/diff": "^7.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.5",
    "@types/should": "^13.0.0",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vscode/vsce": "^2.22.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.56.0",
    "glob": "^7.2.0",
    "minimatch": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "should": "^13.2.3",
    "typescript": "^5.3.3",
    "vscode-test": "^1.6.1"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "chokidar": "^4.0.3",
    "clone-deep": "^4.0.1",
    "default-shell": "^2.2.0",
    "delay": "^6.0.0",
    "diff": "^7.0.0",
    "execa": "^9.5.2",
    "fast-deep-equal": "^3.1.3",
    "globby": "^14.0.2",
    "isbinaryfile": "^5.0.4",
    "mammoth": "^1.9.0",
    "monaco-vscode-textmate-theme-converter": "^0.1.7",
    "node-fetch": "2",
    "openai": "^4.78.1",
    "os-name": "^6.0.0",
    "p-wait-for": "^5.0.2",
    "pdf-parse": "^1.1.1",
    "puppeteer-chromium-resolver": "^24.0.0",
    "puppeteer-core": "^24.0.0",
    "serialize-error": "^12.0.0",
    "strip-ansi": "^7.1.0",
    "tree-sitter-wasms": "^0.1.12",
    "turndown": "^7.2.0",
    "web-tree-sitter": "^0.24.7",
    "zod": "^3.24.1"
  }
}
